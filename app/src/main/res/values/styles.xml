<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/global_color</item>
        <item name="colorPrimaryDark">@color/text_generals</item>
        <item name="android:fontFamily">@font/questrial_regular</item>
        <item name="fontFamily">@font/questrial_regular</item> <!-- target android sdk versions < 26 and > 14 if theme other than AppCompat -->

    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="text_cabeceras">

        <item name="android:textSize">30dp</item>
        <item name="android:textColor">@color/global_color</item>

    </style>


    <style name="textRelog">
        <item name="android:textColor">#1B5C91</item>
        <item name="android:textSize">40dp</item>
    </style>



    <style name="textDia">
        <item name="android:textColor">#1B5C91</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="text_color_button_cvs" parent="@android:style/TextAppearance">
        <item name="android:textColor">#ffffffff</item>
    </style>



</resources>
